<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abdelrhman.realmplayground">


    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".RealmApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".gridview.GridActivity">

        </activity>
        <activity android:name=".threadexample.ThreadExampleActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".threadexample.ReceivingActivity"/>

        <service android:name=".threadexample.ReceivingService"
            android:exported="false" />
        <receiver android:name=".threadexample.WakefulReceivingBroadcastReceiver" />
        <service android:name=".threadexample.WakefulReceivingService" />
    </application>

</manifest>